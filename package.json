{
  "name": "front-platfrom-ui",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Upstars Global",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upstars-global/front-platfrom-ui.git"
  },
  "bugs": {
    "url": "https://github.com/upstars-global/front-platfrom-ui/issues"
  },
  "homepage": "https://github.com/upstars-global/front-platfrom-ui#readme",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/types/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./components/*": "./src/components/*",
    "./assets/*": "./src/assets/*",
    "./plugins/*": "./src/plugins/*"
  },
  "files": [
    "src/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!public/**/*",
    "!script/**/*"
  ],
  "scripts": {
    "dev:storybook": "storybook dev -p 6006",
    "build:package": "vite build -c vite.lib.ts",
    "build:types": "vue-tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:dts": "yarn clean:types && yarn build:types",
    "build:storybook": "storybook build",
    "build:chromatic": "chromatic --build-command='yarn build:storybook' -o='storybook-static' --exit-zero-on-changes",
    "clean:types": "rimraf src/types",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint:tsc": "vue-tsc --noEmit",
    "lintfix": "eslint . --fix && prettier --write --list-different ."
  },
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.5.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "defu": "~6.1.4",
    "tailwindcss": "~3.4.17",
    "vue": "~3.5.18",
    "vue-router": "~4.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.31.0",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-vitest": "9.0.18",
    "@storybook/vue3-vite": "^9.0.18",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "chromatic": "^13.1.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-storybook": "9.0.18",
    "eslint-plugin-vue": "^10.3.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "playwright": "^1.54.1",
    "postcss": "^8.5.6",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.18",
    "tailwind-merge": "^2.6.0",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-component-type-helpers": "^3.0.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.4"
  },
  "peerDependencies": {
    "@vue/tsconfig": "^0.7.0",
    "defu": "^6.1.4",
    "tailwind-merge": "~2.6.0",
    "tailwindcss": "^3.4.x",
    "vue": "^3.5.x",
    "vue-router": "^4.5.x"
  },
  "packageManager": "yarn@1.22.22"
}
