{
  "name": "front-platform-ui",
  "version": "1.6.0",
  "license": "MIT",
  "author": "Upstars Global",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upstars-global/front-platform-ui.git"
  },
  "bugs": {
    "url": "https://github.com/upstars-global/front-platform-ui/issues"
  },
  "homepage": "https://github.com/upstars-global/front-platform-ui#readme",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/types/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./components/*": "./src/components/*",
    "./assets/*": "./src/assets/*",
    "./tailwind-config": "./tailwind.config.ts"
  },
  "files": [
    "src/**/*",
    "README.md",
    "*.d.ts",
    "tailwind.config.ts",
    "!public/**/*",
    "!script/**/*"
  ],
  "scripts": {
    "dev:storybook": "storybook dev -p 6006 --no-open",
    "build:types": "vue-tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:dts": "pnpm run clean:types && pnpm run build:types",
    "build:storybook": "storybook build",
    "build:chromatic": "chromatic --build-command='pnpm run build:storybook' -o='storybook-static' --exit-zero-on-changes",
    "clean:types": "rimraf src/types",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint:tsc": "vue-tsc --noEmit",
    "lintfix": "eslint . --fix && prettier --write --list-different .",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22.19.0"
  },
  "lint-staged": {
    ".github/**/*.yaml": [
      "prettier . --check"
    ],
    "**/*.md": [
      "prettier . --check"
    ],
    "**/*.css": [
      "prettier . --check"
    ],
    "**/*.{js,tsx,vue}": [
      "eslint .",
      "prettier . --check"
    ],
    "**/*.!(d).ts": [
      "eslint .",
      "prettier . --check"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.37.0",
    "@octokit/auth-app": "^8.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-vitest": "9.1.10",
    "@storybook/vue3-vite": "^9.1.10",
    "@types/node": "^22.18.8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "chromatic": "^13.3.0",
    "defu": "^6.1.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.10",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.9",
    "storybook": "^9.1.13",
    "tailwindcss": "3.4.17",
    "tailwind-merge": "2.6.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.8",
    "vitest": "^3.2.4",
    "vue": "~3.5.22",
    "vue-component-type-helpers": "^3.1.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-router": "~4.5.1",
    "vue-tsc": "^3.1.0"
  },
  "peerDependencies": {
    "@vue/tsconfig": "^0.7.0",
    "defu": "^6.1.4",
    "tailwind-merge": "~2.6.0",
    "tailwindcss": "^3.4.x",
    "vue": "^3.5.x",
    "vue-router": "^4.5.x"
  },
  "packageManager": "pnpm@10.17.0"
}